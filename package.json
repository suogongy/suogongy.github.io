{
  "name": "personal-github-page",
  "version": "2.0.0",
  "description": "A modern personal GitHub Page built with Next.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "deploy:vercel": "vercel --prod",
    "deploy:github": "npm run build && touch .nojekyll && git add . && git commit -m 'Deploy to GitHub Pages' && git push origin main"
  },
  "keywords": [
    "nextjs",
    "react",
    "github-pages",
    "vercel",
    "portfolio",
    "personal-website"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "marked": "^9.1.2",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.0",
    "typescript": "^5.2.2",
    "vercel": "^32.5.0"
  }
}
